{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RentalsConnection": "Server= localhost\\sqlexpress; Database=Rentals; User Id=SA; password=My_StrongnewPassword_!123; Integrated Security=False; Encrypt=False; TrustServerCertificate=True"
  },
  "Serilog": {
